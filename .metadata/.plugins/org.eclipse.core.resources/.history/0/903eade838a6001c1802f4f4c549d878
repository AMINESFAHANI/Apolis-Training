


//Method Hiding 

package com.apolis.main;


class Parent{
    static void method() {
       System.out.println("Base Class method()");
   }
}

class Child extends Parent{
       static void method() {
           System.out.println("Child Class method()");
       }
}

public class MethodHiding{
    public static void main(String[] args) {
        Parent.method();
    }
}




